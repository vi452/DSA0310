import spacy

nlp = spacy.load("en_core_web_sm")

def extract_noun_phrases(sentence):
    doc = nlp(sentence)
    results = []
    for chunk in doc.noun_chunks:
        meaning = {
            "text": chunk.text,
            "root": chunk.root.text,
            "head": chunk.root.head.text,
            "modifiers": [tok.text for tok in chunk if tok.dep_ in ("amod", "compound")]
        }
        results.append(meaning)
    return results

sentence = "The intelligent student solved the complex problem."
print(extract_noun_phrases(sentence))
